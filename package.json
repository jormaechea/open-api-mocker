  "directories": {
    "lib": "lib",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://jormaechea@github.com/jormaechea/open-api-mocker.git"
  },
  "bugs": {
    "url": "https://github.com/jormaechea/open-api-mocker/issues"
