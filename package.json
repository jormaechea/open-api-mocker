    "cors": "^2.8.5",
    "express": "^4.17.1",
    "js-yaml": "^4.1.0",
    "json-refs": "^3.0.15",
    "lllog": "^1.1.2",
    "micro-memoize": "^4.0.9",
    "parse-prefer-header": "^1.0.0",
    "superstruct": "^0.6.2",
    "yargs": "^16.2.0"
