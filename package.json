  "author": "Joaquin Ormaechea",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.24.2",
    "husky": "^4.3.8",
    "mocha": "^9.1.2",
    "mock-require": "^3.0.3",
    "nyc": "^15.1.0",
    "sinon": "^11.1.2"
  },
  "dependencies": {
    "@faker-js/faker": "^7.6.0",
    "ajv": "^6.12.6",
    "ajv-openapi": "^2.0.0",
    "body-parser": "^1.19.0",
    "chokidar": "^3.5.2",
    "colors": "^1.4.0",
